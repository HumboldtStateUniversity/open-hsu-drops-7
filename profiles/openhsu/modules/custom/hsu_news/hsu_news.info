name = OpenHSU News
description = Leverages Panopoly News to create a News Article content type for the HSU Distribution.
core = 7.x
package = OpenHSU
version = 7.x-1.41
project = hsu_news
dependencies[] = panopoly_core
dependencies[] = panopoly_news
dependencies[] = views
dependencies[] = views_content
features[ctools][] = strongarm:strongarm:1
features[ctools][] = views:views_default:3.0
features[defaultconfig][] = user_default_permissions:create panopoly_news_article content
features[defaultconfig][] = user_default_permissions:delete any panopoly_news_article content
features[defaultconfig][] = user_default_permissions:delete own panopoly_news_article content
features[defaultconfig][] = user_default_permissions:edit any panopoly_news_article content
features[defaultconfig][] = user_default_permissions:edit own panopoly_news_article content
features[features_api][] = api:2
features[variable][] = date_format_hsu_news_listing
features[views_view][] = hsu_news
features_exclude[dependencies][ctools] = ctools
features_exclude[dependencies][defaultconfig] = defaultconfig
features_exclude[dependencies][strongarm] = strongarm
